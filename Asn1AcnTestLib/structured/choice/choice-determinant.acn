-- ACN encoding with determinant attribute for CHOICE

ChoiceDeterminant DEFINITIONS ::= BEGIN

    MessageType [] {}

    MessagePayload []
    {
        command [],
        telemetry [],
        event [],
        file []
    }

    Message []
    {
        msgType [],
        payload [determinant msgType]
    }

    TestStruct []
    {
        message1 [],
        message2 []
    }

END
